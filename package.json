{
  "name": "idea-bot",
  "type": "module",
  "version": "0.0.1-alpha.1",
  "private": true,
  "description": "idea generation bot using seedcord and gemini api",
  "repository": {
    "type": "git",
    "url": "https://github.com/seedcord/seedcord.git"
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "src/index.ts",
  "files": [],
  "scripts": {
    "dev": "tsx src/bot.ts --conditions=development",
    "dev:bot": "tsx watch src/bot.ts",
    "start:watch": "tsx watch src/bot.ts",
    "start:bot": "node dist/index.js",
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint 'src/**/*.{ts,tsx}' --cache",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix --cache",
    "fmt": "prettier --write 'src/**/*.{ts,tsx,json,md}' --cache",
    "fmt:check": "prettier --check 'src/**/*.{ts,tsx,json,md}' --cache",
    "tc": "tsc --noEmit"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@google/genai": "^1.14.0",
    "discord.js": "^14.21.0",
    "envapt": "^2.2.9",
    "openai": "^5.12.2",
    "reflect-metadata": "^0.2.2",
    "seedcord": "^0.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@seedcord/types": "^0.1.0",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript-eslint": "^8.39.1"
  }
}
